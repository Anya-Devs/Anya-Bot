{
  "name": "anya-bot-website",
  "version": "1.0.0",
  "description": "Anya Bot website with character collection system",
  "type": "module",
  "scripts": {
    "dev": "npm run parse-bot-commands && vite",
    "server": "tsx watch --env-file .env server/index.ts",
    "start": "node --env-file=.env dist/server/index.js",
    "build": "npm run parse-bot-commands && tsc && vite build && tsc -p tsconfig.server.json",
    "preview": "vite preview",
    "copy-commands": "node scripts/copy-commands.js",
    "process-commands": "node scripts/process-commands.js",
    "parse-bot-commands": "node scripts/parse-bot-commands.js",
    "parse-cogs": "node scripts/parse-cogs.js",
    "setup": "node scripts/setup.js",
    "scrape": "node src/scraper/index.js",
    "batch-scrape": "node scripts/batch-scrape.js",
    "deploy": "npm run build && firebase deploy"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.4",
    "axios": "^1.6.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "firebase": "^10.7.1",
    "framer-motion": "^10.16.16",
    "lucide-react": "^0.294.0",
    "mongodb": "^6.3.0",
    "onnxruntime-web": "^1.17.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "tailwind-merge": "^3.4.0",
    "tsx": "^4.7.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/mongodb": "^4.0.6",
    "@types/node": "^20.19.25",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.3.1",
    "firebase-admin": "^12.0.0",
    "firebase-tools": "^13.0.2",
    "image-hash": "^7.0.1",
    "postcss": "^8.4.32",
    "puppeteer": "^24.31.0",
    "sharp": "^0.33.1",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.3",
    "uuid": "^9.0.1",
    "vite": "^6.4.1"
  }
}
